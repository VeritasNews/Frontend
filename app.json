{
  "expo": {
    "name": "VeritasFrontend",
    "slug": "VeritasFrontend",
    "version": "1.0.0",
    "assetBundlePatterns": ["assets/**/*"],
    "plugins": ["expo-font"],
    "newArchEnabled": true,
    "web": {
      "favicon": "./assets/favicon.png",
      "meta": {
        "viewport": "width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no"
      },
      "build": {
        "babel": {
          "include": ["@react-native-web"]
        }
      }
    }
  }
}
